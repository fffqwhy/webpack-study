{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es5", "es2015.promise"],
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
